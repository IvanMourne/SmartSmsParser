  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NPOI" Version="2.6.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Excel\Template\" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Excel\Template\Paymaster import template.xlsx">
